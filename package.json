{
	"name": "storefront_backend",
	"version": "0.1.0",
	"description": "",
	"main": "server.ts",
	"scripts": {
		"start": "node src/server.ts",
		"build": "tsc --esModuleInterop src/server.ts --outDir ./dist",
		"watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
		"test": "ENV=test db-migrate --env test up && jasmine-ts && db-migrate db:drop test",
		"start-DB": "db-migrate up",
		"drop-DB": "db-migrate down"
	},
	"author": "Udacity",
	"license": "ISC",
	"dependencies": {
		"bcrypt": "^5.0.1",
		"body-parser": "^1.19.0",
		"cors": "^2.8.5",
		"db-migrate": "^0.11.13",
		"db-migrate-pg": "^1.2.2",
		"dotenv": "^16.0.0",
		"express": "^4.17.1",
		"jsonwebtoken": "^8.5.1",
		"morgan": "^1.10.0",
		"pg": "^8.5.1",
		"typescript": "^4.1.3"
	},
	"devDependencies": {
		"@types/bcrypt": "^5.0.0",
		"@types/cors": "^2.8.12",
		"@types/express": "^4.17.9",
		"@types/jasmine": "^3.6.3",
		"@types/jsonwebtoken": "^8.5.8",
		"@types/morgan": "^1.9.3",
		"@types/pg": "^7.14.7",
		"jasmine": "^3.6.4",
		"jasmine-spec-reporter": "^6.0.0",
		"jasmine-ts": "^0.3.0",
		"prettier": "2.6.2",
		"ts-node": "^9.1.1",
		"tsc-watch": "^4.2.9"
	}
